    TEAM_STORMTROOPER = DarkRP.createJob("Stormtrooper",{
	color = Color(255, 0, 0, 255),
	model = "models/npc/star wars the force awakens/first order trooper/spartanmark6/first_order_trooper.mdl",
	description = [[Regiment]],
	weapons = {"sw_weapon_first_order_f11d"},
	command = "stormtrooper",
	max = 0,
	salary = 0,
	admin = 0,
	vote = false,
	hasLicense = false,
	category = "Stormtroopers",
	customCheck = function(ply) return ply:GetNWString("usergroup") == "stormtrooper" or ply:IsAdmin() end,
    CustomCheckFailMsg = "You must be whitelisted!"
})

