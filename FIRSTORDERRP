	max = 1,
	salary = 0,
	admin = 0,
	vote = false,
	hasLicense = false,
	category = "Navy",
	customCheck = function(ply) return ply:GetNWString("usergroup") == "captain" or ply:IsAdmin() end,
    CustomCheckFailMsg = "You must be whitelisted!"
})





